    "i18next-resources-to-backend": "^1.2.0",
    "next": "14.2.0-canary.54",
    "next-sitemap": "^4.2.3",
    "next-themes": "0.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.2",
    "react-i18next": "^14.1.0",
    "recharts": "^2.12.3",
