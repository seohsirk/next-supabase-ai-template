    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tailwindcss": "3.4.1",
    "typescript": "^5.4.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@kit/eslint-config/base",
