    "@kit/billing-gateway": "workspace:^",
    "@kit/cms": "workspace:^",
    "@kit/database-webhooks": "workspace:^",
    "@kit/email-templates": "workspace:^",
    "@kit/i18n": "workspace:^",
    "@kit/mailers": "workspace:^",
    "@kit/shared": "workspace:^",
    "@kit/supabase": "workspace:^",
    "@kit/team-accounts": "workspace:^",
    "@kit/ui": "workspace:^",
